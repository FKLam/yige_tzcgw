body {
    background-color: #ededed;
    font-family: "微软雅黑";
    color: #333;
    width: 100%;
    height: 100%;
    font-size: 28rpx;
    word-wrap: break-word;
}

.container {
    width: 100%;
    height: 100%;
}

.clearfix {
    zoom: 1;
}

.clearfix:after {
    display: block;
    overflow: hidden;
    clear: both;
    height: 0;
    visibility: hidden;
    content: ".";
}

image {
    display: block;
    width: 100%;
    border: none;
}

.navigator-hover {
    background-color: rgba(0,0,0,0);
    opacity: 1;
}

.mjlogo {
    width: 100%;
    padding: 20rpx 0;
    position: absolute;
    left: 0;
    bottom: 0;
}

.mjlogo image {
    display: block;
    width: 146rpx;
    height: 48rpx;
    margin: 0 auto;
}

.mjlogo view {
    margin-top: 10rpx;
    font-size: 16rpx;
    color: #c2c2c2;
    text-align: center;
}

.lineNum_1 {
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
    overflow: hidden;
}

.lineNum_2 {
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    overflow: hidden;
}

.lineNum_3 {
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 3;
    overflow: hidden;
}

.lineNum_4 {
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 4;
    overflow: hidden;
}

.lineNum_5 {
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 5;
    overflow: hidden;
}

.version {
    height: 84rpx;
    line-height: 84rpx;
    color: #999999;
    text-align: center;
    font-size: 24rpx;
}
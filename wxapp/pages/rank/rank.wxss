.mjMax {
    min-height: 100%;
    position: relative;
}

.mjMaxTwo {
    padding: 0 0 0 0;
}

.mjBody {
    position: fixed;
    z-index: 10;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
}

.mjBody .bg {
    width: 100%;
    height: 100%;
}

.mjRankingMax {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
}

.mjRankingMax .mjRanking_1 {
    margin: 30rpx 30rpx 0 30rpx;
    color: #887495;
    font-size: 32rpx;
    font-weight: bold;
    background-color: rgba(0,0,0,0.4);
    border-radius: 200rpx;
    height: 64rpx;
    line-height: 64rpx;
    padding: 13rpx;
    display: -webkit-box;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-orient: horizontal;
    -webkit-box-direction: normal;
    -ms-flex-direction: row;
    flex-direction: row;
    -ms-flex-wrap: nowrap;
    flex-wrap: nowrap;
}

.mjRankingMax .mjRanking_1 .box {
    width: 100%;
    text-align: center;
    border-radius: 200rpx;
}

.mjRankingMax .mjRanking_1 .box.select {
    color: #fff;
    background-color: #d705fa;
}

.mjRankingMax .mjRanking_2 {
    display: none;
}

.mjRankingMax .mjRanking_2.select {
    display: block;
}

.mjRankingMax .mjRanking_2 .max {
    margin: 30rpx 30rpx 0 30rpx;
}

.mjRankingMax .mjRanking_2 .max .maxbox {
    height: 750rpx;
}

.mjRankingMax .mjRanking_2 .max .maxbox .box {
    background-color: rgba(0,0,0,0.4);
    border-radius: 8rpx;
    margin-top: 10rpx;
    padding: 20rpx;
}

.mjRankingMax .mjRanking_2 .max .maxbox .box:first-child {
    margin-top: 0;
}

.mjRankingMax .mjRanking_2 .max .maxbox .box .minbox_1 {
    float: left;
    color: #ffd220;
    width: 64rpx;
    height: 64rpx;
    border-radius: 100%;
    text-align: center;
    line-height: 64rpx;
    margin-top: 9rpx;
    font-size: 30rpx;
    font-weight: bold;
    position: relative;
}

.mjRankingMax .mjRanking_2 .max .maxbox .box .minbox_1.select {
    background-color: rgba(255,255,255,0.2);
}

.mjRankingMax .mjRanking_2 .max .maxbox .box .minbox_1 .img {
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%,-50%);
    width: 64rpx;
    height: 60rpx;
}

.mjRankingMax .mjRanking_2 .max .maxbox .box .minbox_2 {
    float: left;
    margin-left: 30rpx;
    width: 82rpx;
    height: 82rpx;
    border: 4rpx solid rgba(255,255,255,0.2);
    border-radius: 100%;
    box-sizing: border-box;
}

.mjRankingMax .mjRanking_2 .max .maxbox .box .minbox_3 {
    float: left;
    margin-left: 20rpx;
    width: 300rpx;
}

.mjRankingMax .mjRanking_2 .max .maxbox .box .minbox_3 .min1 {
    font-size: 28rpx;
    line-height: 30rpx;
    color: #fff;
}

.mjRankingMax .mjRanking_2 .max .maxbox .box .minbox_3 .min2 {
    font-size: 28rpx;
    color: #fff;
    margin-top: 10rpx;
}

.mjRankingMax .mjRanking_2 .max .maxbox .box .minbox_3 .min2 image {
    width: 20rpx;
    height: 40rpx;
    display: inline-block;
    vertical-align: middle;
}

.mjRankingMax .mjRanking_2 .max .maxbox .box .minbox_3 .min2 view {
    display: inline-block;
    vertical-align: middle;
    margin-left: 10rpx;
}

.mjRankingMax .mjRanking_2 .max .maxbox .box .minbox_4 {
    float: right;
    line-height: 82rpx;
    color: #fff;
    font-size: 28rpx;
    font-weight: bold;
}

.mjRankingMax .mjRanking_2 .max .maxbox .box .minbox_4 text {
    font-size: 36rpx;
    color: #0ceaf9;
}

.mjRankingMax .mjRanking_3 {
    margin: 30rpx 30rpx 0 30rpx;
    color: #fff;
    font-size: 28rpx;
    background-color: rgba(255,255,255,0.1);
    border-radius: 8rpx;
    height: 100rpx;
    padding: 0 20rpx;
}

.mjRankingMax .mjRanking_3 .min1 {
    float: left;
    width: 40%;
    line-height: 100rpx;
}

.mjRankingMax .mjRanking_3 .min2 {
    float: left;
    width: 20%;
    line-height: 100rpx;
    text-align: center;
    color: #ffd220;
    font-weight: bold;
}

.mjRankingMax .mjRanking_3 .min3 {
    float: left;
    width: 40%;
    text-align: right;
}

.mjRankingMax .mjRanking_3 .min3 view:nth-child(1) {
    color: #0ceaf9;
    font-size: 30rpx;
    margin-top: 10rpx;
}

.mjRankingMax .mjRanking_3 .min3 view:nth-child(2) {
    font-size: 26rpx;
}

.mjRankingMax .mjRanking_4 {
    width: 400rpx;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    border-radius: 200rpx;
    margin: 0 auto;
    margin-top: 40rpx;
    background-color: #ff1c6c;
    font-weight: bold;
    font-size: 30rpx;
    color: #fff;
    position: relative;
}

.mjRankingMax .mjRanking_4 view {
    display: inline-block;
    vertical-align: middle;
}

.mjRankingMax .mjRanking_4 image {
    display: inline-block;
    vertical-align: middle;
    width: 20rpx;
    height: 40rpx;
    margin-left: 20rpx;
}

.mjRankingMax .mjRanking_4 button {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    opacity: 0;
}
@-webkit-keyframes runGold {
    0% {
        transform: translateY(20rpx);
    }

    100% {
        transform: translateY(-20rpx);
    }
}

@keyframes runGold {
    0% {
        transform: translateY(20rpx);
    }

    100% {
        transform: translateY(-20rpx);
    }
}

